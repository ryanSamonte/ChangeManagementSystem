<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost/ChangeManagementSystem</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/ChangeManagementSystem</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ApplicationDbContext" Order="2" Enabled="True">
          <Destination Path="Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDB;Integrated Security=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDb;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\ApplicationDbContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDb;Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
          <Object Type="DbFullSql">
            <Source Path="grant.sql" Transacted="False" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="cmdDb" Order="3" Enabled="True">
          <Destination Path="Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDB;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="ChangeManagementSystem.Models.ApplicationDbContext, ChangeManagementSystem" MigrationConfiguration="ChangeManagementSystem.Migrations.Configuration, ChangeManagementSystem" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <PrecompileBeforePublish>True</PrecompileBeforePublish>
    <EnableUpdateable>True</EnableUpdateable>
    <DebugSymbols>False</DebugSymbols>
    <WDPMergeOption>DonotMerge</WDPMergeOption>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ApplicationDbContext-Web.config Connection String">
      <ParameterValue>Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDB;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)cmdDb-Web.config Connection String">
      <ParameterValue>Data Source=DESKTOP-TMGDRME\SQLEXPRESS;Initial Catalog=cmdDB;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="cmdDb" />
  </ItemGroup>
